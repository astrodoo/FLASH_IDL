pro trim,mult=mult,recover=recover

if keyword_set(recover) then begin
   spawn,'mv tmp_jet/* .'
   stop
endif

if not keyword_set(mult) then mult=5

fname = file_search('JetSet_hdf5_plt_cnt*')
digit = fix(strmid(fname,3,4,/reverse))

spawn,'mkdir tmp_jet'

fname1 = fname[where((digit mod mult) ne 0)]

for i=0, n_elements(fname1)-1 do begin 
    print,'mv '+fname1[i]+' tmp_jet/    (last: '+fname1[n_elements(fname1)-1]+')'
    spawn,'mv '+fname1[i]+' tmp_jet/'
endfor

stop
end
